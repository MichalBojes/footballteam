body
{
	background-color: rgb(185, 185, 240);
}



#container
{
	margin-left: auto;
	margin-right: auto;
	width: 600px;
	border: solid black;
	background-color: rgb(226, 222, 222);

}


#logo
{
	margin-left:50px;
	margin-right:50px;
	width: 500px;
	font-size: 44px;
	text-align: center;
	margin-bottom: 30px;
}

form#register-user-form {
	max-width: 350px;
	margin: 0 auto;
}

.part
{
	font-size: 16px;
	padding: 5px 0;
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}
